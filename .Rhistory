build.bar.chart <- function(x.var, y.var, x.label, y.label, title) {
p <- plotly(
x = x.var,
y = y.var,
name = title,
type = "bar"
)
return(p)
}
build.bar.chart(x.var = c("Esha", "Kav", "Sarang"),
y.var = c("37", "21", "100"),
x.label = "Names",
y.label = "Temperature in (Celsius)",
title = "Weather by Names")
build.bar.chart(x.var = c("Esha", "Kav", "Sarang"),
y.var = c("37", "21", "100"),
title = "Weather by Names")
build.bar.chart <- function(x.var, y.var, x.label, y.label, title) {
p <- plot_ly(
x = x.var,
y = y.var,
name = title,
type = "bar"
)
return(p)
}
