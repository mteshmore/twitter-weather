build.bar.chart <- function(x.var, y.var, x.label, y.label, title) {
p <- plot_ly(
x = x.var,
y = y.var,
name = title,
type = "bar"
) %>%
layout(
yaxis = list(range = c(0, 200))
)
return(p)
}
